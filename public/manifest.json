{
  "manifest_version": 2,
  "name": "Easy Backlog Opener",
  "version": "0.1.0",
  "description": "You can open issue of Backlog to easy.",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action": {
    "default_title": "Easy Backlog Opener",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": [
    "storage"
  ]
}
